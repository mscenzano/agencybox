<div class="row">
	<div class="col-md-4 offset-4">
	
		<%= form_for @client do |f| %>
		  <%= f.label :name %>:
		  <%= f.text_field :name %>

		  <%= f.label :contact_name %>:
		  <%= f.text_field :contact_name %>

		  <%= f.label :email %>:
		  <%= f.text_field :email %>

		  <%= f.label :phone %>:
		  <%= f.text_field :phone %>

		  <%= f.submit %>
		<% end %>

	</div>
</div>